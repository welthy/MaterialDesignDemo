<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.welthy.materialdesigndemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ToolBarActivity" android:theme="@style/ToolBarTheme"/>
        <activity android:name=".CardViewActivity"/>
        <activity android:name=".CoordinatorActivity"/>
        <activity android:name=".FloatActionBtnActivity"/>
        <activity android:name=".NavigationViewActivity"/>
        <activity android:name=".SnackBarActivity"/>
        <activity android:name=".TablayoutActivity"/>
        <activity android:name=".TextInputLayoutActivity"/>
    </application>

</manifest>